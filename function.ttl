@prefix fno: <http://semweb.datasciencelab.be/ns/function#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix varharnom: <https://github.com/SDM-TIB/VariantHarmonizedNomenclature/blob/master/function.ttl#> .

varharnom:subStringProblem a fno:Problem;
varharnom:subStringAlgorithm a fno:Algorithm;

varharnom:subStringFunction a fno:Function;
    fno:name "The string extraction function"^^xsd:string;
    dcterms:description "This function can extract part of a string base on requirement."^^xsd:string;
    fno:solves varharnom:subStringProblem;
    fno:implements varharnom:subStringAlgorithm;
    fno:expects ( 
        [
            fno:predicate varharnom:stringToLook; 
            fno:required "true"^^xsd:boolean 
        ]
        [
            fno:predicate varharnom:subStringIdentifier; 
            fno:required "true"^^xsd:boolean 
        ]
    ).
    fno:returns (
        [
            fno:predicate varharnom:extractedSubString; 
            fno:required "true"^^xsd:boolean 
        ]  
    ).
varharnom:stringToLook fno:type xsd:string.
varharnom:subStringIdentifier fno:type xsd:string.
varharnom:extractedSubString fno:type xsd:string.

varharnom:subStringExecution a fno:Execution;
    fno:executes varharnom:subStringFunction;
    varharnom:stringToLook ""^^xsd:string;
    varharnom:subStringIdentifier "NC"^^xsd:string.
