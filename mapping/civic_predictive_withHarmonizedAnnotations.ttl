@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms> .
@prefix dce: <http://purl.org/dc/elements/1.1> .
@prefix af: <http://purl.org/ontology/af> .
@prefix ru: <http://purl.org/imbi/ru-meta.owl#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix base: <http://semanticscience.org/resource> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix varharnom: <https://github.com/SDM-TIB/VariantHarmonizedNomenclature/blob/master/function.ttl#> .


<#Evidence>
	rml:logicalSource [ rml:source "/data/data/VariantHarmonizedNomenclature/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011136/{evidence_id}";
		rr:class sio:SIO_011136 # chemical-disease association linked with genomic alteration
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:parentTriplesMap <#Disease> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers to 
		rr:objectMap [ 
			rr:parentTriplesMap <#Publication>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:parentTriplesMap <#Variation> 
		]
	].		
	
<#Variation>
	rml:logicalSource [ rml:source "/data/data/VariantHarmonizedNomenclature/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{variant.clinical}";
		rr:class sio:SIO_011137 # variation
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs; # has annotation
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_011133/{gFormat}"; # gDNA variation notation (function is required)
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs; # has annotation
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_011134/{cFormat}"; # cDNA variation notation (function is required)
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs; # has annotation
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_011135/{pFormat}"; # amino acid sequence variation notation (function is required)
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug1}" # drug
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug2}" # drug
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug3}" # drug
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug4}" # drug
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [ 
			rr:template "http://semanticscience.org/resource/SIO_010038/{drug5}" # drug
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is located in
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_000899/{chromosome.clinical}" # chromosome
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061; # is located in
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{gene.clinical}" # gene
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000061;  # is located in
		rr:objectMap [
			rr:parentTriplesMap <#Transcript>
		]
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001279; # has phenotype
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010056/{phenotypes}" # phenotype 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000628; # refers to
		rr:objectMap [ 
			rr:parentTriplesMap <#Publication>
		]
	].	

<#Disease>
	rml:logicalSource [ rml:source "/data/data/VariantHarmonizedNomenclature/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010299/{disease}"; 
		rr:class sio:SIO_010299 # disease
	].

<#Transcript>	
	rml:logicalSource [ rml:source "/data/data/VariantHarmonizedNomenclature/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_010017/{representative_transcript.clinical}";
		rr:class sio:SIO_010017 # ribonucleic acid sequence
	];
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with
		rr:objectMap [
			rr:template "http://semanticscience.org/resource/SIO_010035/{gene.clinical}" # gene
		]
	].

<#Publication>
	rml:logicalSource [ rml:source "/data/data/VariantHarmonizedNomenclature/civic_Predictive.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_000154/{citation_id}"; 
		rr:class sio:SIO_000154; # publication
	].